package com.ngtech.AbstarctFactoryPattern;

public class DrinksTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		DrinkAbstarctFactory daf=new DrinkAbstarctFactory();
		
		DrinkFactory coldf=((DrinkAbstarctFactory) daf).getDrinksFactory("coldFact");
		Drinks d1=coldf.getDrink("coke");
 		
		DrinkFactory hotf=daf.getDrinksFactory("hotFact");
		Drinks d2=hotf.getDrink("coffee");
 		
	
		d1.drink();
		d2.drink();

	}

}
