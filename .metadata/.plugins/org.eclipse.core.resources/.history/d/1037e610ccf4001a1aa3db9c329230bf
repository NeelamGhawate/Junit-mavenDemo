package com.ngtech.AbstarctFactoryPattern;

public class ColdDrinksFactory implements DrinkFactory
{
	
	
	@Override
	public Drinks getDrink(String ch) {
		// TODO Auto-generated method stub
		 if (ch.equalsIgnoreCase("coke"))
			{
				return new Coke();
			}
			else
			{
				return new Pepsi();
			}
	}
}